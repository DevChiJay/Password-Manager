{
  "expo": {
    "name": "Svault",
    "slug": "svault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "Svault",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Secure vault password manager for mobile devices",
    "privacy": "unlisted",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Svault.app",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Svault uses Face ID for secure and convenient access to your password vault."
      }
    },
    "android": {
      "package": "com.Svault.app",
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#6366f1",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#6366f1",
          "dark": {
            "image": "./assets/images/icon.png",
            "backgroundColor": "#4338ca",
            "imageWidth": 300,
            "resizeMode": "contain"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "795d3a8f-5a88-4787-8d8e-17f0ca58a791"
      }
    },
    "owner": "taichijay"
  }
}
